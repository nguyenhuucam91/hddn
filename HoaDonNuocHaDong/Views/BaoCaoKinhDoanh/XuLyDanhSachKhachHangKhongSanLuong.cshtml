@{
    ViewBag.Title = "XuLyDanhSachKhachHangKhongSanLuong";

}
<link href="~/Content/Style.css" rel="stylesheet" />
<div class="row" style="margin-left:4%">
    <!-- /span6 -->
    <div class="span12">
        <div class="widget">
            <div class="widget-header">
                <i class="icon-bookmark"></i>
                <h3>Tùy chọn báo cáo danh sách khách hàng không sản lượng</h3>
            </div>
            <!-- /widget-header -->
            <div class="widget-content">
                @using (Html.BeginForm("DanhSachKhachHangKhongSanLuong", "BaoCaoKinhDoanh"))
                {
                    <div class="xulybc">          
                        Từ @Html.Partial("FormThangNam")
                        Đến
                        <p>
                            tháng : <input type="number" class="form-control inputd" name="m2" id="m1" min="1" max="12" required>
                            năm: <input type="number" class="form-control inputd" name="y2" id="y1" min="2000" required>
                        </p>


                        <p><input type="submit" class="btn btn-success" value="Xuất báo cáo"></p>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
