@{
    ViewBag.Title = "XuLyDanhSachKhachHangCacLoaiGiaKhac";

}
<link href="~/Content/Style.css" rel="stylesheet" />
<div class="row" style="margin-left:4%">
    <!-- /span6 -->
    <div class="span12">
        <div class="widget">
            <div class="widget-header">
                <i class="icon-bookmark"></i>
                <h3>Tùy chọn báo cáo danh sách khách hàng các loại giá khác</h3>
            </div>
            <!-- /widget-header -->
            <div class="widget-content">
                @using (Html.BeginForm("DanhSachKhachHangCacLoaiGiaKhac", "BaoCaoKinhDoanh"))
                {
                    <div class="xulybc">
                        Loại áp giá: <select name="d2">
                            <option value="@HoaDonNuocHaDong.Helper.KhachHang.SANXUAT">Sản xuất</option>
                            <option value="@HoaDonNuocHaDong.Helper.KhachHang.KINHDOANHDICHVU">Kinh doanh</option>
                            <option value="@HoaDonNuocHaDong.Helper.KhachHang.DONVISUNGHIEP">Công cộng</option>
                            <option value="@HoaDonNuocHaDong.Helper.KhachHang.COQUANHANHCHINH">Hành chính</option>

                        </select>
                        
                        <p><input type="submit" class="btn btn-success" value="Xuất báo cáo"></p>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
