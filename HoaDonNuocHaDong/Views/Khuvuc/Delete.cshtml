@model HoaDonNuocHaDong.To

@{
    ViewBag.Title = "Delete";
Layout = "~/Views/Shared/_LoggedIn.cshtml";
}
<div class="main form-horizontal">
    <div class="main-inner">
        <div class="container">
            <div class="row">
                <div class="span12">
                    <div class="widget ">
                        <div class="widget-header">
                            <i class="icon-user"></i>
                            <h3>Xóa khu vực</h3>
                        </div> <!-- /widget-header -->
                        <div class="widget-content">
                            <div class="tab-pane  active" id="khachhang1">
                                <h3>Are you sure you want to delete this?</h3>
                                <dl class="dl-horizontal">
                                    <dt>
                                        Tên
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.Ten)
                                    </dd>
                                    <dt>
                                        Chi nhánh
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.Chinhanh.Ten)
                                    </dd>
                                    <dt>
                                        Phường
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.Phuongxa.Ten)
                                    </dd>

                                </dl>

                                @using (Html.BeginForm())
                                {
                                    @Html.AntiForgeryToken()

                                    <div class="form-actions no-color">
                                        <input type="submit" value="Delete" class="btn btn-danger" /> |
                                        <button class="btn btn-default">@Html.ActionLink("Quay lại", "Index")</button>
                                    </div>
                                }
                            </div>
                        </div> <!-- /widget-content -->
                    </div> <!-- /widget -->
                </div> <!-- /span8 -->
            </div> <!-- /row -->
        </div> <!-- /container -->
    </div> <!-- /main-inner -->
</div> <!-- /main -->

