@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>

    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">

    <title>Login</title>
    <!-- Styles CSS -->
    @Styles.Render("~/Css/bootstrap.css")
    @Styles.Render("~/Css/bootstrap.min.css")
    @Styles.Render("~/Assets/components/font-awesome/css/font-awesome.min.css")
    @Styles.Render("~/Css/style.css")
    @Styles.Render("~/Css/pages/signin.css")
    @Styles.Render("http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600")
   
</head>
<body>
    <!-- Header (navigation) -->
    @RenderBody()
    <!-- /main -->
    <!-- Javascript file-->
    @Scripts.Render("~/js/jquery-1.7.2.min.js")
    @Scripts.Render("~/js/signin.js")
    @Scripts.Render("~/js/bootstrap.js")
    @RenderSection("scripts", required: false)
</body>
</html>
